
CheckPlaceIDs = function(PlaceIDs)
    if PlaceIDs then
        for _, PlaceID in PlaceIDs do 
            if game.PlaceId == PlaceID then
                return true
            end
        end 
    return false
    end
end

local Info = {
    ShouldDisconnect = false,
    DisconnectReason = "Script under maintenance."
}

return Info
