
CheckPlaceIDs = function(PlaceIDs)
    if PlaceIDs then
        for _, PlaceID in PlaceIDs do 
            if game.PlaceId == PlaceID then
                return true
            end
        end 
    return false
    end
end

local Info = {
    ShouldDisconnect = CheckPlaceIDs({6698800091, 17693782507, 17693765307, 18754149095, 94708661875364},
    DisconnectReason = "Script under maintenance."
}

return Info
